library(bbmle)
sigmoid = function(x,a,d,b,x0){
return((a-d)*(1+exp(-b*x0)))/(1+exp(b*(x-x0)))
}
probability = function(x,a1,b1,x01,d2,b2,x02,c){
return(c * sigmoid(x,a1,1,b1,x01) * sigmoid(x,1,d2,b2,x02))
}
likelihood = function(values,data,a1,b1,x01,d2,b2,x02,c){
sum = 0
for(i in 0:length(values)){
environ = data[i];
isFound = values[i]
sum = sum + -log10(probability(environ,a1,b1,x01,d2,b2,x02,c)^isFound *
(1-probability(environ,a1,b1,x01,d2,b2,x02,c)))
}
return(sum)
}
